{
	"$schema": "https://json.schemastore.org/chrome-manifest",
	"name": "webext-host-permission-toggle",
	"version": "0.0.0",
	"manifest_version": 3,
	"permissions": [
		"activeTab",
		"contextMenus"
	],
	"host_permissions": [
		"https://*.github.com/*"
	],
	"action": {},
	"optional_host_permissions": [
		"*://*/*"
	],
	"background": {
		"service_worker": "../background.js",
		"type": "module"
	}
}
